generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

// exempel p√• table

model Order {
  id          Int       @id @default(autoincrement())  
  user_id     Int       
  products    Json    
  total_amount Float    
  created_at  DateTime  @default(now())
  updated_at  DateTime  @updatedAt 

  @@map("orders")
}